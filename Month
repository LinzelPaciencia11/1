#include <stdio.h>

int main()

{

    unsigned int month;

    //Ask user to input week number

    printf("Enter month number (1-7): ");

    scanf("%u", &month);

    if(month == 1)

    {

        printf("January");

    }

    else if(month == 2)

    {

        printf("Febuary");

    }

    else if(month == 3)

    {

        printf("March");

    }

    else if(month == 4)

    {

        printf("April");

    }

    else if(month == 5)

    {

        printf("May");

    }

    else if(month == 6)

    {

        printf("June");

    }

    else if(month == 7)

    {

        printf("July");

    }

    else

    {

        printf("Invalid Input! Please enter week number between 1-7.");

    }

    return 0;

}
